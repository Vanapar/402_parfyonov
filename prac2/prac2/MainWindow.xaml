<Window x:Class="prac2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
        <TabControl x:Name="Tab_Control" />
        <Button x:Name="button_create_new_tab" Content="New Tab" Width="100" Height="40" Background="LightGray" Click="button_create_new_tab_Click" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
